<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <!--#region UI Colors keys  -->
    <SolidColorBrush x:Key="background" Color="#282A36"/>
    <SolidColorBrush x:Key="primary" Color="#44475A"/>
    <SolidColorBrush x:Key="second" Color="#4286A2"/>
    <SolidColorBrush x:Key="third" Color="#17181E"/>
    <SolidColorBrush x:Key="topbar" Color="#17181E"/>
    <SolidColorBrush x:Key="text" Color="#FFFFFF"/>
    <SolidColorBrush x:Key="error" Color="#B85C5C"/>
    <SolidColorBrush x:Key="shadow" Color="#21222C"/>
    <!--#endregion-->

    <!--#region Controls Colors keys  -->
    <SolidColorBrush x:Key="generic-button" Color="#5CB85C"/>
    <SolidColorBrush x:Key="cancel-button" Color="#B85C5C"/>
    <SolidColorBrush x:Key="accept-button" Color="#5CB85C"/>
    <SolidColorBrush x:Key="background-textbox" Color="#21222C"/>
    <SolidColorBrush x:Key="alternative-background-textbox" Color="#1b1c24"/>
    <SolidColorBrush x:Key="ScrollBar" Color="#4286A2"/>
    <SolidColorBrush x:Key="datagrid-selct-background" Color="#27465d"/>
    <SolidColorBrush x:Key="datagrid-selct-border" Color="#26a0da"/>
    <!--#endregion-->

</ResourceDictionary>